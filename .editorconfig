root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false
insert_final_newline = false
indent_size = 4

[package.json]
indent_size = 2
insert_final_newline = false
